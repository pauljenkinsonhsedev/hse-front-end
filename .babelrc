{
   "presets": [
      [
         "@babel/preset-env",
         {
            "useBuiltIns": "usage",
            "corejs": "3",
            "targets": {
               "browsers": [
                  "last 5 versions",
                  "ie >= 11"
               ]
            }
         }
      ]
   ],
   "plugins": [
      "@babel/plugin-transform-arrow-functions"
   ]
}